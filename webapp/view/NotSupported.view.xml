<mvc:View
	controllerName="hcm.ux.hapv4.controller.NotSupported"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">

	<MessagePage
		title="{i18n>notSupportedTitle}"
		text="{i18n>notSupportedText}"
		icon="sap-icon://desktop-mobile"
		id="idNotSupportedPage"
		description="">
		<customDescription>
			<Link id="link" text="{i18n>backToWorklist}" press=".onLinkPressed" />
		</customDescription>
	</MessagePage>

</mvc:View>